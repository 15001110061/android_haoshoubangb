<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="style_rl_buttombar">
        <item name="android:layout_height">@dimen/dd_dimen_120px</item>
        <item name="android:layout_width">@dimen/dd_dimen_0px</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">@dimen/dd_dimen_10px</item>
    </style>

    <style name="style_ib_buttombar">
        <item name="android:layout_height">@dimen/dd_dimen_120px</item>
        <item name="android:layout_width">@dimen/dd_dimen_110px</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="RadiogroupheadboyTheme" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/selector_head_boy_btn</item>
    </style>
    <!-- <item name="android:button">@drawable/selector_head_boy_btn</item> -->
    <style name="RadiogroupheadgirlTheme" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/selector_radio_head_checkbox</item>
    </style>

    <style name="MyPopupAnimation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@drawable/push_bottom_in</item>
        <item name="android:windowExitAnimation">@drawable/push_bottom_out</item>
    </style>

    <style name="ButtomBar">
        <item name="android:layout_height">@dimen/dd_dimen_96px</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="ButtomBarImgBtn">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">@dimen/dd_dimen_96px</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/btn_xheckbox_rect_selector</item>
    </style>
<style name="CustomCheckboxTheme2" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/btn_xheckbox_rect_selector2</item>
    </style>
</resources>