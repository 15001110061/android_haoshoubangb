<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_seting_two"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/common_grey" >

    <include
        android:id="@+id/title"
        layout="@layout/stub_title" />

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/title" >
    </RelativeLayout>

    <com.cgzz.job.b.view.ReboundScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/relativeLayout1"
        android:layout_marginTop="@dimen/dd_dimen_20px"
        android:fadingEdge="none"
        android:scrollbars="none" >

        <LinearLayout
            android:id="@+id/ll_help_1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/rl_addprice_1"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dd_dimen_96px"
                android:layout_marginLeft="@dimen/dd_dimen_32px"
                android:layout_marginRight="@dimen/dd_dimen_32px"
                android:layout_marginTop="@dimen/dd_dimen_20px"
                android:background="@drawable/shape_current_item_et_bg" >

                <TextView
                    android:id="@+id/tv_help_biaojianjiage1"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dd_dimen_25px"
                    android:gravity="center_vertical"
                    android:text="每人打赏:"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/dd_dimen_32px" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dd_dimen_25px"
                    android:scaleType="fitXY"
                    android:src="@drawable/icons_arrow" />

                <TextView
                    android:id="@+id/tv_addprice_danjia1"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/tv_help_biaojianjiage1"
                    android:gravity="center_vertical"
                    android:text="0"
                    android:textColor="@color/common_yellow"
                    android:textSize="@dimen/dd_dimen_32px" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_toRightOf="@+id/tv_addprice_danjia1"
                    android:gravity="center_vertical"
                    android:text="元"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/dd_dimen_32px" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dd_dimen_32px"
                android:layout_marginRight="@dimen/dd_dimen_32px"
                android:layout_marginTop="@dimen/dd_dimen_20px" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dd_dimen_25px"
                    android:gravity="center_vertical"
                    android:text="共需打赏:"
                    android:textColor="@color/common_666666"
                    android:textSize="@dimen/dd_dimen_26px" />

                <TextView
                    android:id="@+id/tv_help_biaojianjiage8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:gravity="center_vertical"
                    android:text="0"
                    android:textColor="@color/common_yellow"
                    android:textSize="@dimen/dd_dimen_26px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:gravity="center_vertical"
                    android:text="元"
                    android:textColor="@color/common_666666"
                    android:textSize="@dimen/dd_dimen_26px" />
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/rl_addprice_2"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dd_dimen_96px"
                android:layout_marginLeft="@dimen/dd_dimen_32px"
                android:layout_marginRight="@dimen/dd_dimen_32px"
                android:layout_marginTop="@dimen/dd_dimen_20px"
                android:background="@drawable/shape_current_item_et_bg" >

                <TextView
                    android:id="@+id/tv_help_biaojianjiage2"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dd_dimen_25px"
                    android:gravity="center_vertical"
                    android:text="标间价格:"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/dd_dimen_32px" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dd_dimen_25px"
                    android:scaleType="fitXY"
                    android:src="@drawable/icons_arrow" />

                <TextView
                    android:id="@+id/tv_addprice_danjia2"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/tv_help_biaojianjiage2"
                    android:gravity="center_vertical"
                    android:text="0"
                    android:textColor="@color/common_yellow"
                    android:textSize="@dimen/dd_dimen_32px" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_toRightOf="@+id/tv_addprice_danjia2"
                    android:gravity="center_vertical"
                    android:text="元/间"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/dd_dimen_32px" />
            </RelativeLayout>

            

            <RelativeLayout
                android:id="@+id/rl_addprice_3"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dd_dimen_96px"
                android:layout_marginLeft="@dimen/dd_dimen_32px"
                android:layout_marginRight="@dimen/dd_dimen_32px"
                android:layout_marginTop="@dimen/dd_dimen_20px"
                android:background="@drawable/shape_current_item_et_bg" >

                <TextView
                    android:id="@+id/tv_help_biaojianjiage3"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dd_dimen_25px"
                    android:gravity="center_vertical"
                    android:text="套间价格:"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/dd_dimen_32px" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dd_dimen_25px"
                    android:scaleType="fitXY"
                    android:src="@drawable/icons_arrow" />

                <TextView
                    android:id="@+id/tv_addprice_danjia3"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/tv_help_biaojianjiage3"
                    android:gravity="center_vertical"
                    android:text="0"
                    android:textColor="@color/common_yellow"
                    android:textSize="@dimen/dd_dimen_32px" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_toRightOf="@+id/tv_addprice_danjia3"
                    android:gravity="center_vertical"
                    android:text="元/间"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/dd_dimen_32px" />
            </RelativeLayout>

            

            <TextView
                android:id="@+id/tv_help_zhaobangke2"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dd_dimen_96px"
                android:layout_marginBottom="@dimen/dd_dimen_30px"
                android:layout_marginLeft="@dimen/dd_dimen_60px"
                android:layout_marginRight="@dimen/dd_dimen_60px"
                android:layout_marginTop="@dimen/dd_dimen_64px"
                android:background="@drawable/shape_home_item_tv_sign_bg"
                android:gravity="center"
                android:text="提交"
                android:textColor="@color/common_white"
                android:textSize="@dimen/dd_dimen_36px" />
        </LinearLayout>
    </com.cgzz.job.b.view.ReboundScrollView>

    <RelativeLayout
        android:id="@+id/ll"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical"
        android:visibility="gone" >
    </RelativeLayout>

</RelativeLayout>